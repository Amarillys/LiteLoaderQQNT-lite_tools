{
    "manifest_version": 2,
    "type": "extension",
    "name": "轻量工具箱",
    "slug": "lite_tools",
    "description": "LiteLoaderQQNT的轻量工具箱",
    "version": "0.1.3",
    "thumbnail": "./icon.png",
    "author": {
        "name": "曦月",
        "link": "https://github.com/xiyuesaves"
    },
    "repository": {
        "repo": "xiyuesaves/lite_tools",
        "branch": "main"
    },
    "platform": [
        "win32",
        "linux",
        "darwin"
    ],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    }
}